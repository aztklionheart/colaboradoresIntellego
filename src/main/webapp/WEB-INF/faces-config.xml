<?xml version="1.0" encoding="UTF-8"?>

<faces-config xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd" version="2.0">

	<application>
		<locale-config>
			<default-locale>es_MX</default-locale>
		</locale-config>
		<resource-bundle>
			<base-name>bundles.messages</base-name>
			<var>msg</var>
		</resource-bundle>
	</application>
<!-- 	<factory> -->
<!-- 		<exception-handler-factory>mx.gob.tab.sigitab.portal.util.CustomExceptionHandlerFactory</exception-handler-factory> -->
<!-- 	</factory> -->
	<navigation-rule>
		<from-view-id>*</from-view-id>
		<navigation-case>
			<from-outcome>home</from-outcome>
			<to-view-id>/views/home/home.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>
</faces-config>
